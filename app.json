{
  "expo": {
    "name": "sudoku-test",
    "slug": "sudoku-test",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/sudoku.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "/assets/demiurge.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/sudoku.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.emanuele.sudoku"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/demiurge.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "fe0a88a3-326b-48cd-ae7f-eb6b35ca94c9"
      }
    }
  }
}
